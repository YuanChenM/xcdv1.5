<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.msk.product.logic.PD141128Logic">
    <!--
        添加记录
    -->
   <insert parameterType="com.msk.product.bean.PD141128Param" id="findSaveMarket">
      insert into pd_tc_market(
      TC_ONLINE_ID
     ,CLASSES_CODE
     ,MACHINING_CODE
     ,BREED_CODE
     ,LEVEL_CODE
     ,LEVEL_NAME
     ,FEATURE_CODE
     ,FEATURE_NAME
     ,LOCAL_NAME
     ,FEATURE_FLG
     ,TREE_LEVEL
     ,DEL_FLG
     ,CRT_ID
     ,CRT_TIME
     <!--Modify: 添加公共属性 2016/09/20   BY  任强  Start -->
     ,UPD_ID
     ,UPD_TIME
     ,ACT_ID
     ,ACT_TIME
     <!--Modify: 添加公共属性 2016/09/20   BY  任强  End -->
     ,VER
      )values(
      #{tcOnlineId}
     ,#{classesCode}
     ,#{machiningCode}
     ,#{breedCode}
     ,#{levelCode}
     ,#{levelName}
     ,#{featureCode}
     ,#{featureName}
     ,#{localName}
     ,#{featureFlg}
     ,#{treeLevel}
     ,#{delFlg}
     ,#{crtId}
     ,#{crtTime}
     <!--Modify: 添加公共属性 2016/09/20   BY  任强  Start -->
     ,#{updId}
     ,#{updTime}
     ,#{actId}
     ,#{actTime}
     ,0
     <!--Modify: 添加公共属性 2016/09/20   BY  任强  End -->
       )
   </insert>

    <!--
        查询记录是否存在
    -->
    <select id="findQueryMarket" parameterType="BaseParam" resultType="int">
        select count(1)
        from pd_tc_market
        <where>
            <if test="filterMap.classesCode!=null and filterMap.classesCode!=''">
                and CLASSES_CODE=#{filterMap.classesCode}
            </if>
            <if test="filterMap.machiningCode!=null and filterMap.machiningCode!=''">
                and MACHINING_CODE=#{filterMap.machiningCode}
            </if>
            <if test="filterMap.breedCode!=null and filterMap.breedCode!=''">
                and BREED_CODE=#{filterMap.breedCode}
            </if>
            <if test="filterMap.featureName!=null and filterMap.featureName!=''">
                and FEATURE_NAME=#{filterMap.featureName}
            </if>
            <if test="filterMap.delFlg!=null and filterMap.delFlg!=''">
                and DEL_FLG=#{filterMap.delFlg}
            </if>
        </where>
    </select>
</mapper>
    <!--保存数据-->
    <insert id="saveData" parameterType="com.msk.core.entity.DsPrintTask">
        <!-- 应上海需求，新加四个属性值-->
        INSERT INTO ds_print_task (
	    TABLE_ID,
	    PRINT_TASK_ID,
	    OPERATE_BAR,
	    READ_BAR,
        BRAND_NAME,
        GRADE_NAME,
        PRODUCT_NAME,
        FEATURE_NAME,
        SUPP_NAME,
        NET_WEIGHT,
        DEL_FLG,
        CRT_ID,
        CRT_TIME,
        UPD_ID,
        UPD_TIME,
        VER
        )values(
          #{tableId},
          #{printTaskId},
          #{operateBar},
          #{readBar},
          #{brandName},
          #{gradeName},
          #{productName},
          #{featureName},
          #{suppName},
          #{netWeight},
          #{delFlg},
          #{crtId},
          #{crtTime},
          #{updId},
          #{updTime},
          #{ver}
        )
    </insert>

